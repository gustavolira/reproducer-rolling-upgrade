{
  "distributed-cache": {
    "mode": "SYNC",
    "persistence": {
      "remote-store": {
        "protocol-version": "2.5",
        "shared": true,
        "hotrod-wrapping": true,
        "raw-values": true,
        "cache": "default",
        "segmented": false,
        "security": {
          "authentication": {
            "server-name": "infinispan",
            "digest": {
              "username": "user",
              "password": "passwd-123",
              "realm": "default",
              "sasl-mechanism": "DIGEST-MD5"
            }
          }
        },
        "remote-server": {
          "host": "127.0.0.1",
          "port": 11222
        }
      }
    }
  }
}